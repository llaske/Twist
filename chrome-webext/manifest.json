{
	"manifest_version": 2,
	"name": "Twist",
	"version": "0.0.1",
	"author": "Lionel Lask√©",
	"description": "Add on for Twist tool",
	"homepage_url": "http://twist.red",
	"icons": {
		"48": "icons/twist-48.png",
		"96": "icons/twist-96.png"
	},

	"permissions": [
		"alarms",
		"tabs"
